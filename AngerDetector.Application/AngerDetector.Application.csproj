<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyName>AngerDetector</AssemblyName>
    <StartupObject>AngerDetector.App</StartupObject>
    <ApplicationIcon>email.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="email.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AngerDetector.Service\AngerDetector.Service.csproj" />
  </ItemGroup>

</Project>
